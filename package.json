{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js",
    "dev:build": "cross-env NODE_ENV=development webpack --config config/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cli-templates/template-react.git"
  },
  "keywords": [
    "react",
    "template"
  ],
  "author": "biaodigit",
  "license": "ISC",
  "homepage": "https://github.com/cli-templates/template-react#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    {{#with jest}}
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    {{#with typescript}}
    "@types/jest": "^24.9.1",
    {{/with}}
    {{/with}}
    {{#with typescript}}
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    {{/with}}
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^10.0.0",
    {{#with jest}}
    "camelcase": "^6.2.0",
    {{/with}}
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "fork-ts-checker-webpack-plugin": "^5.0.5",
    "html-webpack-plugin": "^4.3.0",
    {{#with jest}}
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.1",
    {{/with}}
    {{#with less}}
    "less-loader": "^6.1.2",
    {{/with}}
    "mini-css-extract-plugin": "^0.9.0",
    {{#with sass}}
    "node-sass": "^4.14.1",
    {{/with}}
    "postcss-loader": "^3.0.0",
    "react-app-polyfill": "^2.0.0",
    "react-hot-loader": "^4.12.21",
    {{#with redux}}
    "redux-devtools": "^3.5.0",
    {{/with}}
    {{#with sass}}
    "sass-loader": "^8.0.2",
    {{/with}}
    "style-loader": "^1.2.1",
    {{#with stylus}}
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    {{/with}}
    {{#with typescript}}
    "ts-loader": "^7.0.5",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    {{/with}}
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    {{#if redux}}
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    {{#if react_router}}
    "react-router-dom": "^5.2.0",
    {{/if}}
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
    {{else}}
    "react": "^16.13.1",
    "react-dom": "^16.13.1"{{#if react_router}},
    "react-router-dom": "^5.2.0"
    {{/if}}
    {{/if}}
  },
  {{#with jest}}
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/__tests__"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.{{js_suffix}}"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "./node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  {{/with}}
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}